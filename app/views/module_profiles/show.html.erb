<%- model_class = ModuleProfile -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idmodulo) %>:</strong></dt>
  <dd><%= @module_profile.idmodulo %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idprofile) %>:</strong></dt>
  <dd><%= @module_profile.idprofile %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              module_profiles_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_module_profile_path(@module_profile), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              module_profile_path(@module_profile),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>

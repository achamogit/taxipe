  <script type="text/javascript">
    var map;
    $(document).ready(function(){
      prettyPrint();
      map = new GMaps({
        div: '#tmap',
        lat: -12.097028,
        lng: -77.024316
      });
      map.drawRoute({
        origin: [-12.097028, -77.024316],
        destination: [-11.956457, -76.734915],
        travelMode: 'driving',
        strokeColor: '#cc0000',
        strokeOpacity: 0.6,
        strokeWeight: 6
      });
    });
  </script>

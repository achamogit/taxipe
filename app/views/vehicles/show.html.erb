<%- model_class = Vehicle -%>
<div class="page-header">
    <table>
    <tr>
      <td  style="width:60px"><img src="<%= asset_path 'vehiculos.png' %>"  style="width:50px"></td>
      <td><h1>Vehicles<h1></td>
    </tr>
  </table>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:vehicle_brand_id) %>:</strong></dt>
  <dd><%= VehicleBrand.find(@vehicle.vehicle_brand_id).name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vehicle_model_id) %>:</strong></dt>
  <dd><%= VehicleModel.find(@vehicle.vehicle_model_id).name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:placa) %>:</strong></dt>
  <dd><%= @vehicle.placa %></dd>
  <dt><strong><%= model_class.human_attribute_name(:year) %>:</strong></dt>
  <dd><%= @vehicle.year %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numberseat) %>:</strong></dt>
  <dd><%= @vehicle.numberseat %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vehicle_type_id) %>:</strong></dt>
  <dd><%= VehicleType.find(@vehicle.vehicle_type_id).name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state_id) %>:</strong></dt>
  <dd><%= State.find(@vehicle.state_id).name %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              vehicles_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_vehicle_path(@vehicle), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              vehicle_path(@vehicle),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
